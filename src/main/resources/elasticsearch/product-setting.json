{
  "analysis": {
    "tokenizer": {
      "my_nori_tokenizer": {
        "type": "nori_tokenizer",
        "decompound_mode": "mixed"
      },
      "jaso_index_tokenizer": {
        "type": "jaso_tokenizer",
        "mistype": true,
        "chosung": true
      }
    },
    "filter": {
      "suggest_filter": {
        "type": "edge_ngram",
        "min_gram": 1,
        "max_gram": 50
      }
    },
    "analyzer": {
      "jaso_analyzer": {
        "type": "custom",
        "tokenizer": "jaso_index_tokenizer",
        "filter": [
          "icu_normalizer",
          "suggest_filter"
        ]
      },
      "nori_analyzer": {
        "type": "custom",
        "tokenizer": "my_nori_tokenizer",
        "filter": [
          "icu_normalizer"
        ]
      }
    }
  }
}